
{% extends 'templates/default.twig' %}

{% block title %} Group {% endblock %}

{% block current %} Pencarian {% endblock %}
{% block js %}
<script src="https://adminlte.io/themes/AdminLTE/bower_components/select2/dist/js/select2.full.min.js"></script>
<link rel="stylesheet" href="https://adminlte.io/themes/AdminLTE/bower_components/select2/dist/css/select2.min.css">
{% endblock %}
{% block content %}
<div class="container">
  <h2>Modal Example</h2>
  <!-- Trigger the modal with a button -->
  <a data-toggle="modal" data-target="#myModal">Open Modal</button>

  <!-- Modal -->
  <div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog" style=" margin: auto; width: auto;">

      <!-- Modal content-->
     <div class="container">
          <h2>Carousel Example</h2>
          <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
              <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
              <li data-target="#myCarousel" data-slide-to="1"></li>
              <li data-target="#myCarousel" data-slide-to="2"></li>
              <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div style="margin:auto; max-width:82%; max-height:75%; border:3px solid #cfd0ce;" class="carousel-inner">
              <div class="item active">
                <img src="http://miriadna.com/desctopwalls/images/max/The-narrow-road.jpg" align="middle" alt="Los Angeles" style="width:auto;  max-height:700px">
              </div>
              <div style="align:middle"class="item">
                  <center>
                <img src="http://darrekderstinephotography.com/photos/full/Falls_I.jpg" align="middle" alt="Los Angeles" style="width:auto; max-height:600px;">
                </center>
              </div>
              <div class="item">
                <img src="http://miriadna.com/desctopwalls/images/max/Tuscany-fields.jpg" alt="Chicago" style="width:auto;">
              </div>

              <div class="item">
                <img src="https://static.boredpanda.com/blog/wp-content/uploads/2015/11/reflection-landscape-photography-jaewoon-u-fb.jpg" alt="New york" style="width:100%;">
              </div>
            </div>

            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
              <span class="glyphicon glyphicon-chevron-left"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" data-slide="next">
              <span class="glyphicon glyphicon-chevron-right"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
        </div>

    </div>
  </div>

</div>

{% endblock %}
